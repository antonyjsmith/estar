{
  "scripts": {
    "css": "node-sass --output-style expanded --indent-type tab --indent-width 1 sass/style.scss -o .",
    "postcss": "postcss style.css --use autoprefixer --no-map -o style.css --autoprefixer.browsers=\"last 2 versions\"",
    "watch": "node-sass --output-style expanded --indent-type tab --indent-width 1 -w sass -r -o .",
    "zip": "git archive -o estar.zip --prefix=estar/ HEAD",
    "start": "npm run css"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "node-sass": "^4.13.1",
    "postcss-cli": "^7.1.0"
  }
}
